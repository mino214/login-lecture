<body>
    <section class="login-form">
        <h1>CISW LOGIN</h1>
        <form>
            <div class="int-area">
                <input type="text" name="id" id="id">
                <label for="id">STUDENT ID</label> 
            </div>

            <div class="int-area">
                <input type="password" name="psword" id="psword">
                <label for="psword">PASSWORD</label> 
            </div>

            <div class="btn-area">
                <button id="btn"
                 type="submit">LOGIN</button>
            </div>
        </form>
</body>

